includes:
    - vendor/symplify/phpstan-extensions/config/config.neon
    - vendor/phpstan/phpstan/conf/bleedingEdge.neon

parameters:
    tmpDir: /tmp
    paths:
        - src

    # The level 8 is the highest level
    level: 8

    ignoreErrors:
        - 
            identifier: missingType.iterableValue
        - 
            identifier: missingType.generics